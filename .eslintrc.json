{
  "env": {
    "node": true,
    "es2022": true,
    "jest": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/unbound-method": "off"
  },
  "ignorePatterns": [
    "coverage",
    "dist",
    "node_modules",
    "jest.config.ts"
  ]
}
